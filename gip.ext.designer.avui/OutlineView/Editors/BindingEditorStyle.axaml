<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:Controls="clr-namespace:gip.ext.designer.avui.Controls"
    xmlns:PropertyGrid="clr-namespace:gip.ext.designer.avui.PropertyGrid" 
    xmlns:OutlineView="clr-namespace:gip.ext.designer.avui.OutlineView">

    <Style TargetType="{x:Type OutlineView:BindingEditor}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type OutlineView:BindingEditor}">
                    <StackPanel>
                        <ContentControl Content="{Binding Path=Mode.Editor}" />
                        <ContentControl Content="{Binding Path=StringFormat.Editor}" />
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>