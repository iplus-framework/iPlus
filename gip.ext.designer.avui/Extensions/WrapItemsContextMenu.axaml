<ContextMenu x:Class="gip.ext.designer.avui.Extensions.WrapItemsContextMenu"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Translation="clr-namespace:gip.ext.design.avui.Designer">
	<MenuItem Header="{Binding WrapInCanvas, Source={x:Static Translation:Translations.Instance}}" Click="Click_WrapInCanvas">
        <MenuItem.Icon>
            <Image Width="16" Source="avares://gip.ext.designer.avui/Images/canvas.png" />
        </MenuItem.Icon>
    </MenuItem>
        <MenuItem Header="{Binding WrapInGrid, Source={x:Static Translation:Translations.Instance}}" Click="Click_WrapInGrid">
		<MenuItem.Icon>
			<Image Source="avares://gip.ext.designer.avui/Images/grid.png" />
		</MenuItem.Icon>
	</MenuItem>
</ContextMenu>
