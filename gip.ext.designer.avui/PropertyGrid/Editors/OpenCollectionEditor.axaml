<UserControl
	x:Class="gip.ext.designer.avui.Editors.OpenCollectionEditor" 
	xmlns="https://github.com/avaloniaui" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Grid>
		<TextBlock
			Margin="0,0,20,0"
			Text="{Binding DesignerValue}" 
			VerticalAlignment="Center" />
		<Button
			Width="15"
			Margin="0,0,1,0"
			HorizontalAlignment="Right"
			VerticalAlignment="Stretch"
			Content="..."
			Focusable="False"
			Click="open_Click" />
	</Grid>
</UserControl>