<UserControl 
    x:Class="gip.ext.designer.avui.PropertyGrid.Editors.ComboBoxEditor"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ComboBox
        Name="PART_ComboBox"
        BorderThickness="0"
        SelectedItem="{Binding Value}"
        Background="{x:Null}"
        Focusable="False"
        />
</UserControl>
