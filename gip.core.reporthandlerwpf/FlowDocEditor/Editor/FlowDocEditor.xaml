                                <Image Height="32" Width="32" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/selectall32.png" />
                            </Fluent:SplitButton.LargeIcon>
                            <Fluent:SplitButton.Icon>
                                <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/selectall16.png" />
                            </Fluent:SplitButton.Icon>
                            <Fluent:SplitButton.ToolTip>
                                <Fluent:ScreenTip Title="Select All (Ctrl + A)" Image="Images\Edit\selectall48.png" Text="Select all"/>
                            </Fluent:SplitButton.ToolTip>
                            <Fluent:MenuItem Name="SelectParagraphMenuItem" Header="Select Paragraph" Click="SelectParagraphMenuItem_Click">
                                <Fluent:MenuItem.Icon>
                                    <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/selectparagraph16.png" />
                                </Fluent:MenuItem.Icon>
                                <Fluent:MenuItem.ToolTip>
                                    <Fluent:ScreenTip Title="Select Paragraph" Text="Select current paragraph" Image="Images/Edit/selectparagraph48.png" />
                                </Fluent:MenuItem.ToolTip>
                            </Fluent:MenuItem>
                            <Fluent:MenuItem Name="SelectLineMenuItem" Header="Select Line" Click="SelectLineMenuItem_Click">
                                <Fluent:MenuItem.Icon>
                                    <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/selectline16.png" />
                                </Fluent:MenuItem.Icon>
                                <Fluent:MenuItem.ToolTip>
                                    <Fluent:ScreenTip Title="Select Line" Text="Select current line" Image="Images/Edit/selectline48.png" />
                                </Fluent:MenuItem.ToolTip>
                            </Fluent:MenuItem>
                        </Fluent:SplitButton>
                        <Fluent:SplitButton Name="FindButton" Header="Find" Click="FindButton_Click">
