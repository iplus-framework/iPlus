                                <Fluent:MenuItem.Icon>
                                    <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/copyparagraph16.png" />
                                </Fluent:MenuItem.Icon>
                                <Fluent:MenuItem.ToolTip>
                                    <Fluent:ScreenTip Title="Copy Paragraph" Text="Copy current paragraph" Image="Images/Edit/copyparagraph48.png" />
                                </Fluent:MenuItem.ToolTip>
                            </Fluent:MenuItem>
                            <Fluent:MenuItem Name="CopyLineMenuItem" Header="Copy Line" Click="CopyLineMenuItem_Click">
