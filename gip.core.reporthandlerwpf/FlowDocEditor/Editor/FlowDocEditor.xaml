                        <Image Height="16" Width="16" Source="Images/Tab/home16.png" Margin="-1,-1,0,0" />
                    </Fluent:RibbonTabItem.Header>
                    <Fluent:RibbonTabItem.ToolTip>
                        <Fluent:ScreenTip Title="Home" Text="Common Commands" Image="Images/Tab/home48.png" />
                    </Fluent:RibbonTabItem.ToolTip>
                    <Fluent:RibbonTabItem.ContextMenu>
                        <Fluent:ContextMenu>
                            <Fluent:Gallery ScrollViewer.VerticalScrollBarVisibility="Hidden"
                                        Orientation="Vertical"
                                        GroupBy="Tag"
                                        Selectable="False">
                                <Fluent:MenuItem Name="ShowCommonEditGroupMenuItem" Header="Edit" Tag="Customize Home Tab" Click="ShowCommonEditGroupMenuItem_Click" />
                                <Fluent:MenuItem Name="ShowCommonViewGroupMenuItem" Header="View" Tag="Customize Home Tab" Click="ShowCommonViewGroupMenuItem_Click" />
                                <Fluent:MenuItem Name="ShowCommonInsertGroupMenuItem" Header="Insert" Tag="Customize Home Tab" Click="ShowCommonInsertGroupMenuItem_Click" />
