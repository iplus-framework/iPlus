                                <Image Height="32" Width="32" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/find32.png" />
                            </Fluent:SplitButton.LargeIcon>
                            <Fluent:SplitButton.Icon>
