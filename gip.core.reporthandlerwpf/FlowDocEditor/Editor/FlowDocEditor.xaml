                                </Fluent:MenuItem.Icon>
                                <Fluent:MenuItem.ToolTip>
                                    <Fluent:ScreenTip Title="Cut Line" Text="Cut current line" Image="Images/Edit/cutline48.png" />
                                </Fluent:MenuItem.ToolTip>
                            </Fluent:MenuItem>
                        </Fluent:SplitButton>
                        <Fluent:SplitButton Name="CopyButton" Header="Copy" SizeDefinition="Middle,Middle,Small" Click="CopyMenuItem_Click">
