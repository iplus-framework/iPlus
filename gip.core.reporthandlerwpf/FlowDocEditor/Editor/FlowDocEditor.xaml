                    <Fluent:QuickAccessMenuItem Target="{Binding ElementName=NewButton}" IsChecked="True">
                        <Fluent:QuickAccessMenuItem.Icon>
                            <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Document/new16.png" />
                        </Fluent:QuickAccessMenuItem.Icon>
                    </Fluent:QuickAccessMenuItem>
                    <Fluent:QuickAccessMenuItem Target="{Binding ElementName=OpenMenuItem}" IsChecked="True">
                        <Fluent:QuickAccessMenuItem.Icon>
                            <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Document/open16.png" />
                        </Fluent:QuickAccessMenuItem.Icon>
                    </Fluent:QuickAccessMenuItem>
                    <Fluent:QuickAccessMenuItem Target="{Binding ElementName=SaveMenuItem}" IsChecked="True">
                        <Fluent:QuickAccessMenuItem.Icon>
                            <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Document/save16.png" />
                        </Fluent:QuickAccessMenuItem.Icon>
                    </Fluent:QuickAccessMenuItem>
                    <Fluent:QuickAccessMenuItem Target="{Binding ElementName=PrintMenuItem}" IsChecked="True">
                        <Fluent:QuickAccessMenuItem.Icon>
                            <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Document/print16.png" />
                        </Fluent:QuickAccessMenuItem.Icon>
                    </Fluent:QuickAccessMenuItem>
                </Fluent:Ribbon.QuickAccessItems>
