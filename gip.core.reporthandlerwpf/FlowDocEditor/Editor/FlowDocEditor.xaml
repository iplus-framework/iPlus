            <Grid Name="MainGrid" Margin="0,-2,0,0">
                <TabControl Name="TabCell" Margin="0,0,-4,17" BorderThickness="0,0,0,0" PreviewMouseDown="TabCell_PreviewMouseDown" PreviewMouseUp="TabCell_PreviewMouseUp" SelectionChanged="TabCell_SelectionChanged">
                    <TabControl.ItemContainerStyle>
                        <Style TargetType="{x:Type TabItem}">
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TabControl}}, Path=Items.Count}" Value="1">
                                    <Setter Property="Visibility" Value="Collapsed"/>
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </TabControl.ItemContainerStyle>
                    <TabControl.Background>
                        <SolidColorBrush />
                    </TabControl.Background>
                </TabControl>
                <Fluent:StatusBar Name="StatusBar" HorizontalAlignment="Stretch" VerticalAlignment="Bottom" MouseDoubleClick="StatusbarInfo_MouseDoubleClick">
                    <Fluent:StatusBarItem Title="Lines" HorizontalAlignment="Left">
                        <StackPanel Orientation="Horizontal">
                            <vb:VBTextBlock Name="LinesTextBlock" ACCaption="en{'Line: '}de{'Zeile: '}"/>
                            <TextBlock Name="LinesTextBlock1" Text="{Binding SelectedDocument.Editor.SelectedLineNumber, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type p:FlowDocEditor}}}" />
                            <vb:VBTextBlock Name="LinesTextBlock2" ACCaption="en{' of '}de{' von '}"/>
                            <TextBlock Name="LinesTextBlock3" Text="{Binding SelectedDocument.Editor.LineCount, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type p:FlowDocEditor}}}" />
                        </StackPanel>
                    </Fluent:StatusBarItem>
                    <Fluent:StatusBarItem Title="Columns" HorizontalAlignment="Left">
                        <StackPanel Orientation="Horizontal">
                            <vb:VBTextBlock Name="ColumnsTextBlock" ACCaption="en{'Column: '}de{'Spalte: '}"/>
                            <TextBlock Name="ColumnsTextBlock1"  Text="{Binding SelectedDocument.Editor.SelectedColumnNumber, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type p:FlowDocEditor}}}" />
                            <vb:VBTextBlock Name="ColumnsTextBlock2"  ACCaption="en{' of '}de{' von '}"/>
                            <TextBlock Name="ColumnsTextBlock3"  Text="{Binding SelectedDocument.Editor.ColumnCount, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type p:FlowDocEditor}}}" />
                        </StackPanel>
                    </Fluent:StatusBarItem>
                    <Separator HorizontalAlignment="Left" />
                    <Fluent:StatusBarItem Title="Word Count" HorizontalAlignment="Left">
                        <StackPanel Orientation="Horizontal">
                            <vb:VBTextBlock Name="WordCountTextBlock" ACCaption="en{'Word count: '}de{'Wortzahl: '}"/>
                            <TextBlock Name="WordCountTextBlock1" Text="{Binding SelectedDocument.Editor.WordCount, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type p:FlowDocEditor}}}" />
                        </StackPanel>
                    </Fluent:StatusBarItem>
                    <Separator HorizontalAlignment="Left" />
                    <Fluent:StatusBarItem Title="File Size" HorizontalAlignment="Left">
                        <TextBlock Name="FileSizeTextBlock" Text="File Size" />
                    </Fluent:StatusBarItem>
                    <Separator HorizontalAlignment="Left" />
                    <Fluent:StatusBarItem Name="ZoomTextBlock" Title="Zoom"
                                  HorizontalAlignment="Right"
                                  Value="{Binding Value, ElementName=zoomSlider, StringFormat={}{0:0}\%}" />
                    <Fluent:StatusBarItem Title="Zoom Slider"
                                  HorizontalAlignment="Right">
                        <Slider x:Name="zoomSlider"
                        Minimum="20"
                        Maximum="500"
                        IsDirectionReversed="False"
                        Style="{DynamicResource ZoomSliderStyle}"
                        Value="100" 
                        ValueChanged="zoomSlider_ValueChanged"/>
                    </Fluent:StatusBarItem>
                </Fluent:StatusBar>
            </Grid>
        </DockPanel>
    </Grid>
</UserControl>
