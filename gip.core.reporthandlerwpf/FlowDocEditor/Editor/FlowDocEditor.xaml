                                <Image Height="32" Width="32" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/undo32.png" />
                            </Fluent:Button.LargeIcon>
                            <Fluent:Button.ToolTip>
                                <Fluent:ScreenTip Title="Undo (Ctrl + Z)" Image="Images\Edit\undo48.png" Text="Undo"/>
                            </Fluent:Button.ToolTip>
                        </Fluent:Button>
                        <Fluent:Button Name="RedoButton" Icon="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/redo32.png" Header="Redo" SizeDefinition="Large" Click="RedoMenuItem_Click">
                            <Fluent:Button.LargeIcon>
                                <Image Height="32" Width="32" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/redo32.png" />
                            </Fluent:Button.LargeIcon>
                            <Fluent:Button.ToolTip>
                                <Fluent:ScreenTip Title="Redo (Ctrl + Y)" Image="Images\Edit\redo48.png" Text="redo"/>
