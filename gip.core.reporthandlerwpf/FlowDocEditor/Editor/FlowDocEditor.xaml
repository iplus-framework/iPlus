                            <Fluent:SplitButton.Icon>
                                <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/cut16.png" />
                            </Fluent:SplitButton.Icon>
                            <Fluent:SplitButton.ToolTip>
                                <Fluent:ScreenTip Title="Cut (Ctrl + X)" Image="Images\Edit\cut48.png" Text="Cut to clipboard"/>
                            </Fluent:SplitButton.ToolTip>
