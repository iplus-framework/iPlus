                                <Fluent:MenuItem.Icon>
                                    <Image Height="16" Width="16" Source="/gip.core.reporthandlerwpf;Component/FlowDocEditor/Editor/Images/Edit/cutparagraph16.png" />
                                </Fluent:MenuItem.Icon>
                                <Fluent:MenuItem.ToolTip>
                                    <Fluent:ScreenTip Title="Cut Paragraph" Text="Cut current paragraph" Image="Images/Edit/cutparagraph48.png" />
                                </Fluent:MenuItem.ToolTip>
