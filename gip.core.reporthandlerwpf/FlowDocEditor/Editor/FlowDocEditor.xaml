                            </Fluent:SplitButton.Icon>
                            <Fluent:SplitButton.ToolTip>
                                <Fluent:ScreenTip Title="Find (Ctrl + F)" Image="Images\Edit\find48.png" Text="Find text"/>
                            </Fluent:SplitButton.ToolTip>
                            <Fluent:MenuItem Name="ReplaceButton" Header="Replace" Click="ReplaceButton_Click">
