                                <Fluent:MenuItem Name="ShowCommonToolsGroupMenuItem" Header="Tools" Tag="Customize Home Tab" Click="ShowCommonToolsGroupMenuItem_Click" />
                            </Fluent:Gallery>
                        </Fluent:ContextMenu>
                    </Fluent:RibbonTabItem.ContextMenu>
