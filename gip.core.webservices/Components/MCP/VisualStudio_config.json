{
  "mcpServers": {
    "iPlus": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "http://localhost:8750/sse",
        "--allow-http"
      ]
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_TOKEN>"
      }
    }
  }
}