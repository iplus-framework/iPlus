<ResourceDictionary
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:gip.core.layoutengine"
   xmlns:OutlineView="clr-namespace:gip.ext.designer.OutlineView;assembly=gip.ext.designer"
   xmlns:PropertyGrid="clr-namespace:gip.ext.designer.PropertyGrid;assembly=gip.ext.designer"
   xmlns:Controls="clr-namespace:gip.ext.designer.Controls;assembly=gip.ext.designer">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/gip.core.layoutengine;Component/Controls/VBDesignEditor/Themes/ActionCollectionEditorStyleGip.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="ActionCollectionEditorStyleAero" 
           TargetType="{x:Type local:VBActionCollectionEditor}" 
           BasedOn="{StaticResource ActionCollectionEditorStyleGip}">
    </Style>
</ResourceDictionary>
