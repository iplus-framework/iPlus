<ResourceDictionary
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero"
   xmlns:vb="clr-namespace:gip.core.layoutengine">
    <Style x:Shared="False" 
           x:Key="{vb:VBResourceKey ACUrlWPF=\\VBTextBox, ACUrlProperty=\\DBAManager\\Material\\ACClassProperty(MaterialName1)}" 
           TargetType="{x:Type vb:VBTextBox}">
        <Style.Setters>
            <Setter Property="FontSize">
                <Setter.Value>
                    <vb:VBBinding VBContent="LokalDouble1"/>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
</ResourceDictionary>
