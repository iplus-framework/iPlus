<ResourceDictionary
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:gip.core.layoutengine">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/gip.core.layoutengine;Component/Controls/Shared.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="PageStyleGip" TargetType="{x:Type local:VBPage}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Name="MainGridMobile" Margin="0,0,0,0">
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="-1,-1,0,-1"/>
    </Style>
    
</ResourceDictionary>
