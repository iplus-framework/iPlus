<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:gip.core.layoutengine.avui">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/gip.core.layoutengine.avui;Component/Controls/Shared.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="PageStyleGip" TargetType="{x:Type local:VBPage}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Name="MainGridMobile" Margin="0,0,0,0">
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="-1,-1,0,-1"/>
    </Style>
    
</ResourceDictionary>
