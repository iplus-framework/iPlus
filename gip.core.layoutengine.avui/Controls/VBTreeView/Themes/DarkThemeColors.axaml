<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   x:ClassModifier="internal">
    <RadialGradientBrush x:Key="TreeArrowFocusFill" Center="50%,50%" RadiusX="100%" GradientOrigin="50%,50%" RadiusY="100%">
        <GradientStop Color="#FFE99393" Offset="0.228"/>
        <GradientStop Color="#FFFF0018" Offset="1"/>
    </RadialGradientBrush>
    <RadialGradientBrush x:Key="TreeArrowCheckedFill" Center="50%,50%" RadiusX="100%" GradientOrigin="50%,50%" RadiusY="100%">
        <GradientStop Color="#FF505050"/>
        <GradientStop Color="White" Offset="0.732"/>
    </RadialGradientBrush>
    <LinearGradientBrush x:Key="TreeViewItemSelectionFill" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FF828282" Offset="0"/>
        <GradientStop Color="#FF825555" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="TreeViewItemSelectionNotActiveFill" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#FF828282" Offset="0"/>
        <GradientStop Color="#FF825555" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="TreeViewItemSelectionFg" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="TreeViewItemFg" Color="#FFFFFFFF"/>
</ResourceDictionary>
