<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   x:ClassModifier="internal">

  <SolidColorBrush x:Key="TextEditorBgEnabled" Color="White"/>
  <SolidColorBrush x:Key="TextEditorFgEnabled" Color="Black"/>
  <SolidColorBrush x:Key="TextEditorFgEnabledFocus" Color="Black"/>
  <RadialGradientBrush x:Key="TextEditorBorderEnabled" Center="85%,90%" RadiusX="90%" GradientOrigin="80%,80%" RadiusY="80%" >
    <GradientStop Color="#FFC5C5C6" Offset="0.208"/>
    <GradientStop Color="#FF666666" Offset="0.548"/>
    <GradientStop Color="Black" Offset="1"/>
  </RadialGradientBrush>

  <SolidColorBrush x:Key="TextAreaSelectionBorder" Color="DodgerBlue"/>
  <SolidColorBrush x:Key="TextAreaSelectionBg" Color="Cyan"/>
  <SolidColorBrush x:Key="TextAreaSelectionFg" Color="Black"/>
  <SolidColorBrush x:Key="TextAreaBgEnabled" Color="#FF1A1A1A"/>
  <SolidColorBrush x:Key="TextAreaStatusBarBorder" Color="Gray"/>
  
</ResourceDictionary>
