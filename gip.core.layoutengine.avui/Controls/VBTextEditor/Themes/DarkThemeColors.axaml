<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   x:ClassModifier="internal">
  
  <SolidColorBrush x:Key="TextEditorBgEnabled" Color="#FF1A1A1A"/>
  <SolidColorBrush x:Key="TextEditorFgEnabled" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="TextEditorFgEnabledFocus" Color="#FFFFFFFF"/>
  <RadialGradientBrush x:Key="TextEditorBorderEnabled" Center="0.85,0.9" RadiusX="0.9" GradientOrigin="0.8,0.8" RadiusY="0.8" >
    <GradientStop Color="#FFC5C5C6" Offset="0.208"/>
    <GradientStop Color="#FF666666" Offset="0.548"/>
    <GradientStop Color="Black" Offset="1"/>
  </RadialGradientBrush>

  <SolidColorBrush x:Key="TextAreaSelectionBorder" Color="DodgerBlue"/>
  <SolidColorBrush x:Key="TextAreaSelectionBg" Color="Cyan"/>
  <SolidColorBrush x:Key="TextAreaSelectionFg" Color="Black"/>
  <SolidColorBrush x:Key="TextAreaBgEnabled" Color="White"/>
  <SolidColorBrush x:Key="TextAreaStatusBarBorder" Color="Gray"/>

</ResourceDictionary>
