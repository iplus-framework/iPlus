                         Stretch="Fill" Fill="{StaticResource ConnStateStrokePath}"/>
                <Ellipse x:Name="Pin1" Width="2" Height="2" Canvas.Left="19" Canvas.Top="15" 
                         Stretch="Fill" Fill="{StaticResource ConnStateStrokePath}"/>
                <Path x:Name="ConnectionUpArrow" Width="9" Height="15" Canvas.Left="1" Canvas.Top="4" 
                      Stretch="Fill" StrokeMiterLimit="2.75" Stroke="{StaticResource ConnStateStrokePath}" 
                      Data="F1 M 4,19L 4,9L 2,9L 6,5L 10,9L 8,9L 8,19L 4,19 Z "/>
            </Canvas>
        </Viewbox>
    </ControlTemplate>

    <ControlTheme x:Key="{x:Type local:VBConnectionState}" TargetType="local:VBConnectionState">
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="Template" Value="{StaticResource TemplateConnectionDown}"/>

        <!-- IsServerConnection = True -->
        <Style Selector="^[IsServerConnection=True]">
            <Setter Property="Template" Value="{StaticResource TemplateConnectionUp}"/>
        </Style>

        <!-- ConnectionQuality states for down connection (default template) -->
        <Style Selector="^[ConnectionQuality=0] /template/ Path#ConnectionDownArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillNoConnection}"/>
        </Style>
        <Style Selector="^[ConnectionQuality=0] /template/ Path#ConnectionDownPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillNoConnection}"/>
        </Style>

        <Style Selector="^[ConnectionQuality=1] /template/ Path#ConnectionDownArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillGood}"/>
        </Style>
        <Style Selector="^[ConnectionQuality=1] /template/ Path#ConnectionDownPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillGood}"/>
        </Style>

        <Style Selector="^[ConnectionQuality=2] /template/ Path#ConnectionDownArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillInstable}"/>
        </Style>
        <Style Selector="^[ConnectionQuality=2] /template/ Path#ConnectionDownPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillInstable}"/>
        </Style>

        <Style Selector="^[ConnectionQuality=3] /template/ Path#ConnectionDownArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillBad}"/>
        </Style>
        <Style Selector="^[ConnectionQuality=3] /template/ Path#ConnectionDownPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillBad}"/>
        </Style>

        <!-- ConnectionQuality states for up connection (IsServerConnection=True) -->
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=0] /template/ Path#ConnectionUpArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillNoConnection}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=0] /template/ Ellipse#ConnectionUpPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillNoConnection}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=0] /template/ Ellipse#Pin1">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePath}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=0] /template/ Ellipse#Pin2">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePath}"/>
        </Style>

        <Style Selector="^[IsServerConnection=True][ConnectionQuality=1] /template/ Path#ConnectionUpArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillGood}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=1] /template/ Ellipse#ConnectionUpPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillGood}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=1] /template/ Ellipse#Pin1">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePathInverse}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=1] /template/ Ellipse#Pin2">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePathInverse}"/>
        </Style>

        <Style Selector="^[IsServerConnection=True][ConnectionQuality=2] /template/ Path#ConnectionUpArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillInstable}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=2] /template/ Ellipse#ConnectionUpPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillInstable}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=2] /template/ Ellipse#Pin1">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePathInverse}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=2] /template/ Ellipse#Pin2">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePathInverse}"/>
        </Style>

        <Style Selector="^[IsServerConnection=True][ConnectionQuality=3] /template/ Path#ConnectionUpArrow">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillBad}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=3] /template/ Ellipse#ConnectionUpPlug">
            <Setter Property="Fill" Value="{StaticResource ConnStateFillBad}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=3] /template/ Ellipse#Pin1">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePathInverse}"/>
        </Style>
        <Style Selector="^[IsServerConnection=True][ConnectionQuality=3] /template/ Ellipse#Pin2">
            <Setter Property="Fill" Value="{StaticResource ConnStateStrokePathInverse}"/>
        </Style>
    </ControlTheme>
</ResourceDictionary>
