                <Path x:Name="_30_Punkt__63" Width="6.75836" Height="7.84375" Canvas.Left="13.6104" Canvas.Top="2.07813" 
                      Stretch="Fill" StrokeLineCap="Round" StrokeLineJoin="Round" 
                      Stroke="{StaticResource ConnStateStrokePath}" 
                      Data="F1 M 14.2875,2.57813C 14.2875,3.32993 14.038,4.08213 14.1312,4.82813C 14.1867,5.27198 14.3562,5.81558 14.7562,6.01563C 15.2451,6.26005 15.8924,6.0413 16.3812,5.79688C 17.0664,5.4543 17.7551,4.77165 18.5062,4.92188C 18.8496,4.99055 19.2115,5.09755 19.475,5.32813C 19.8221,5.63189 19.8687,6.11685 19.8687,6.57813C 19.8687,6.76935 19.7888,7.11597 19.6875,7.27813C 19.502,7.57493 19.41,7.67439 19.1625,7.92188C 18.7456,8.3388 18.4887,8.8945 18.225,9.42188"/>
