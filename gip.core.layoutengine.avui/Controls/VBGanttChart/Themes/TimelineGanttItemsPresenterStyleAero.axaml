<ResourceDictionary xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:gip.core.layoutengine.avui.ganttchart"
    xmlns:timeline="clr-namespace:gip.core.layoutengine.avui.timeline">
    <Style x:Key="TimelineGenttItemsPresenterStyleAero" TargetType="{x:Type local:TimelineGanttItemsPresenter}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:TimelineGanttItemsPresenter}">
                    <Border Background="{TemplateBinding Background}">
                        <Grid>
                            <ItemsPresenter Margin="0,3,0,0" />
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>