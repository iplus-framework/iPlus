        <ResourceDictionary Source="/gip.core.layoutengine.avui;Component/Controls/VBDesignEditor/Themes/DesignItemTreeViewStyleGip.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="DesignItemTreeViewStyleAero" 
