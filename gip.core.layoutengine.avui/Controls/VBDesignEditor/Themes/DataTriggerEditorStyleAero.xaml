   xmlns:PropertyGrid="clr-namespace:gip.ext.designer.avui.PropertyGrid;assembly=gip.ext.designer.avui"
   xmlns:Controls="clr-namespace:gip.ext.designer.avui.Controls;assembly=gip.ext.designer.avui">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/gip.core.layoutengine.avui;Component/Controls/VBDesignEditor/Themes/DataTriggerEditorStyleGip.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="DataTriggerEditorStyleAero" 
           TargetType="{x:Type local:VBDataTriggerEditor}" 
           BasedOn="{StaticResource DataTriggerEditorStyleGip}">
    </Style>
</ResourceDictionary>
