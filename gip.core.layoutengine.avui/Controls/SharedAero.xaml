                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorCtrlBackDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}}"/>

    <Color x:Key="{ComponentResourceKey ResourceId=ColorCtrlBackEnabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
           A="#FF" R="#B3" G="#B3" B="#B3"/>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=CtrlBackEnabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}"
