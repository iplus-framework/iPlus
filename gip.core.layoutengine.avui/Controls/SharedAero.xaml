                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorAppLight, TypeInTargetAssembly={x:Type local:VBCustomControl}}}" />


    <Color x:Key="{ComponentResourceKey ResourceId=TabItemSelectedBorderColorAero, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
           A="#FF" R="#92" G="#92" B="#92"/>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=TabItemSelectedBorderBrushAero, TypeInTargetAssembly={x:Type local:VBCustomControl}}" Color="{StaticResource {ComponentResourceKey ResourceId=TabItemSelectedBorderColorAero, TypeInTargetAssembly={x:Type local:VBCustomControl}}}"/>

    <!--<LinearGradientBrush x:Key="ComboBoxItemSelectionFillAero" EndPoint="0,1" StartPoint="0,0">
        <GradientStop Color="#4CFF6565" Offset="0"/>
        <GradientStop Color="#7FFF0018" Offset="1"/>
    </LinearGradientBrush>
    <Border x:Key="ComboBoxSelectedItemTemplate" CornerRadius="2" Background="{StaticResource ComboBoxItemSelectionFillAero}" BorderBrush="#FF0000">
        <StackPanel x:Name="ComboBoxSelectedItemPanel" Orientation="Horizontal"></StackPanel>
    </Border>-->

    <DrawingBrush x:Key="ConverterErrorBrush" Viewport="0,0,0.25,0.25" TileMode="Tile">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <GeometryDrawing>
                    <GeometryDrawing.Brush>
                        <SolidColorBrush Color="#88FFFFFF"/>
                    </GeometryDrawing.Brush>
                    <GeometryDrawing.Geometry>
                        <RectangleGeometry Rect="0,0,20,20" />
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>

                <GeometryDrawing>
                    <GeometryDrawing.Geometry>
                        <GeometryGroup>
                            <RectangleGeometry Rect="0,0,10,10" />
                            <RectangleGeometry Rect="10,10,10,10" />
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                    <GeometryDrawing.Brush>
                        <SolidColorBrush Color="#88FF0000"/>
                    </GeometryDrawing.Brush>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>

</ResourceDictionary>





