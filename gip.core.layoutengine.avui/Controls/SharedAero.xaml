<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:gip.core.layoutengine.avui">

    <Color x:Key="{ComponentResourceKey ResourceId=ColorCtrlTxtDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
           A="#FF" B="#30" G="#30" R="#30"/>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=CtrlTxtDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}"
                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorCtrlTxtDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}}"/>

    <Color x:Key="{ComponentResourceKey ResourceId=ColorCtrlTxtEnabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
           A="#FF" B="#00" G="#00" R="#00"/>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=CtrlTxtEnabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorCtrlTxtEnabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}}"/>

    <Color x:Key="{ComponentResourceKey ResourceId=ColorCtrlTxtWrong, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
           A="#FF" B="#00" G="#00" R="#00"/>
