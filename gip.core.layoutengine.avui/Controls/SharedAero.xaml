           A="#FF" R="#B3" G="#B3" B="#B3"/>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=CtrlBackRequired, TypeInTargetAssembly={x:Type local:VBCustomControl}}"
                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorCtrlBackRequired, TypeInTargetAssembly={x:Type local:VBCustomControl}}}" />



    <Color x:Key="{ComponentResourceKey ResourceId=ColorCtrlFrmDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}" 
           A="#FF" B="#B3" G="#B3" R="#B3"/>
    <SolidColorBrush x:Key="{ComponentResourceKey ResourceId=CtrlFrmDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}"
                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorCtrlFrmDisabled, TypeInTargetAssembly={x:Type local:VBCustomControl}}}" />

