                     Color="{StaticResource {ComponentResourceKey ResourceId=ColorCtrlTxtWrong, TypeInTargetAssembly={x:Type local:VBCustomControl}}}"/>

    <Color x:Key="{ComponentResourceKey ResourceId=ColorCtrlTxtRequired, TypeInTargetAssembly={x:Type local:VBCustomControl}}"  
           A="#FF" B="#00" G="#00" R="#00"/>
