<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   x:ClassModifier="internal">

  <LinearGradientBrush x:Key="DateTimePickerPopupDarkBorderBrush" EndPoint="50%,100%" StartPoint="50%,0%">
    <GradientStop Color="#FFA3AEB9" Offset="0"/>
    <GradientStop Color="#FF8399A9" Offset="0.375"/>
    <GradientStop Color="#FF718597" Offset="0.375"/>
    <GradientStop Color="#FF617584" Offset="1"/>
  </LinearGradientBrush>

  <LinearGradientBrush x:Key="DateTimePickerPopupBackgroundBrush" StartPoint="0%,0%" EndPoint="0%,100%">
    <LinearGradientBrush.GradientStops>
        <GradientStop Offset="0" Color="#FFFFFFFF"/>
        <GradientStop Offset="1" Color="#FFE8E8E8"/>
    </LinearGradientBrush.GradientStops>
  </LinearGradientBrush>
  
  <SolidColorBrush x:Key="DateTimePickerFillArrowGlyph" Color="White"/>

</ResourceDictionary>
