<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:Microsoft_Windows_Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero"
   xmlns:local="clr-namespace:gip.core.layoutengine.avui">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Controls/DarkThemeColors.axaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="CarouselStyleAero" TargetType="{x:Type local:VBCarousel}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:VBCarousel}">
                    <Grid>
                        <local:VBCarouselControl x:Name="PART_VBCarouselControl" Width="1350" Height="0" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                        </local:VBCarouselControl>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>   
    </Style>

</ResourceDictionary>