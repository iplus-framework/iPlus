	<LinearGradientBrush x:Key="ButtonIdleBackground" StartPoint="0%,0%" EndPoint="0%,100%">
		<GradientStop Offset="0" Color="{StaticResource ButtonIdleBackgroundColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonIdleBackgroundColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonIdleBackgroundColor2}" />
