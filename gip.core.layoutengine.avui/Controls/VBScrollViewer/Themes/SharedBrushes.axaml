		<GradientStop Offset="0" Color="{StaticResource ButtonHoverBackgroundColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonHoverBackgroundColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonHoverBackgroundColor2}" />
		<GradientStop Offset="1" Color="{StaticResource ButtonHoverBackgroundColor3}" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="ButtonHoverOuterBorderBrush" Color="{StaticResource ButtonHoverOuterBorderBrushColor}" />

	<LinearGradientBrush x:Key="ButtonHoverInnerBorderBrush" StartPoint="0%,0%" EndPoint="0%,100%">
		<GradientStop Offset="0" Color="{StaticResource ButtonHoverInnerBorderBrushColor0}" />
		<GradientStop Offset="1" Color="{StaticResource ButtonHoverInnerBorderBrushColor1}" />
	</LinearGradientBrush>


	<LinearGradientBrush x:Key="ButtonPressedBackground" StartPoint="0%,0%" EndPoint="0%,100%">
		<GradientStop Offset="0" Color="{StaticResource ButtonPressedBackgroundColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonPressedBackgroundColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonPressedBackgroundColor2}" />
		<GradientStop Offset="1" Color="{StaticResource ButtonPressedBackgroundColor3}" />
