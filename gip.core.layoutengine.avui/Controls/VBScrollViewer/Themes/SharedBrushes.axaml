	<SolidColorBrush x:Key="ButtonDisabledInnerBorderBrush" Color="{StaticResource ButtonDisabledInnerBorderBrushColor}" />
	<SolidColorBrush x:Key="ButtonDisabledForeground" Color="{StaticResource ButtonDisabledForegroundColor}" />

</ResourceDictionary>
