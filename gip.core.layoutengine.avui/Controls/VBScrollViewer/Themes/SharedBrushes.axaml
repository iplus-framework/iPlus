		<GradientStop Offset="0" Color="{StaticResource ButtonPressedInnerBorderBrushColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonPressedInnerBorderBrushColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource ButtonPressedInnerBorderBrushColor2}" />
		<GradientStop Offset="1" Color="{StaticResource ButtonPressedInnerBorderBrushColor3}" />
