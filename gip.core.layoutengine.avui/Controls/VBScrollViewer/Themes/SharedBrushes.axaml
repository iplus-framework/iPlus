<ResourceDictionary
xmlns="https://github.com/avaloniaui"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
x:ClassModifier="internal">
	<LinearGradientBrush x:Key="ScrollBarButtonForeground" StartPoint="0%,0%" EndPoint="100%,100%">
		<GradientStop Offset="0" Color="{StaticResource ScrollBarButtonForegroundColor0}" />
		<GradientStop Offset="1" Color="{StaticResource ScrollBarButtonForegroundColor1}" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="ScrollBarButtonIconStroke" Color="{StaticResource ScrollBarButtonIconStrokeColor}" />
	<SolidColorBrush x:Key="ScrollBarThumbIconStroke" Color="{StaticResource ScrollBarThumbIconStrokeColor}" />

	<GradientStops x:Key="ScrollBarTrackBackgroundStops">
		<GradientStop Offset="0" Color="{StaticResource ScrollBarTrackBackgroundColor0}" />
		<GradientStop Offset="0.25" Color="{StaticResource ScrollBarTrackBackgroundColor1}" />
		<GradientStop Offset="1" Color="{StaticResource ScrollBarTrackBackgroundColor2}" />
	</GradientStops>
	<LinearGradientBrush x:Key="HorizontalScrollBarTrackBackground" StartPoint="0%,0%" EndPoint="0%,100%"
						 GradientStops="{StaticResource ScrollBarTrackBackgroundStops}" />
	<LinearGradientBrush x:Key="VerticalScrollBarTrackBackground" StartPoint="0%,0%" EndPoint="100%,0%"
						 GradientStops="{StaticResource ScrollBarTrackBackgroundStops}" />

	<GradientStops x:Key="ScrollBarTrackBorderBrushStops">
		<GradientStop Offset="0" Color="{StaticResource ScrollBarTrackBorderBrushColor0}" />
		<GradientStop Offset="1" Color="{StaticResource ScrollBarTrackBorderBrushColor1}" />
	</GradientStops>
	<LinearGradientBrush x:Key="HorizontalScrollBarTrackBorderBrush" StartPoint="0%,0%" EndPoint="0%,100%"
						 GradientStops="{StaticResource ScrollBarTrackBorderBrushStops}" />
	<LinearGradientBrush x:Key="VerticalScrollBarTrackBorderBrush" StartPoint="0%,0%" EndPoint="100%,0%"
						 GradientStops="{StaticResource ScrollBarTrackBorderBrushStops}" />


	<LinearGradientBrush x:Key="VerticalScrollBarButtonIdleBackground" StartPoint="0%,0%" EndPoint="100%,0%">
		<GradientStop Offset="0" Color="{StaticResource VerticalScrollBarButtonIdleBackgroundColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource VerticalScrollBarButtonIdleBackgroundColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource VerticalScrollBarButtonIdleBackgroundColor2}" />
		<GradientStop Offset="1" Color="{StaticResource VerticalScrollBarButtonIdleBackgroundColor3}" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="VerticalScrollBarButtonIdleOuterBorderBrush" Color="{StaticResource VerticalScrollBarButtonIdleOuterBorderBrushColor}" />

	<LinearGradientBrush x:Key="VerticalScrollBarButtonIdleInnerBorderBrush" StartPoint="0%,0%" EndPoint="100%,0%">
		<GradientStop Offset="0" Color="{StaticResource VerticalScrollBarButtonIdleInnerBorderBrushColor0}" />
		<GradientStop Offset="1" Color="{StaticResource VerticalScrollBarButtonIdleInnerBorderBrushColor1}" />
	</LinearGradientBrush>


	<LinearGradientBrush x:Key="VerticalScrollBarButtonHoverBackground" StartPoint="0%,0%" EndPoint="100%,0%">
		<GradientStop Offset="0" Color="{StaticResource VerticalScrollBarButtonHoverBackgroundColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource VerticalScrollBarButtonHoverBackgroundColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource VerticalScrollBarButtonHoverBackgroundColor2}" />
		<GradientStop Offset="1" Color="{StaticResource VerticalScrollBarButtonHoverBackgroundColor3}" />
	</LinearGradientBrush>

	<SolidColorBrush x:Key="VerticalScrollBarButtonHoverOuterBorderBrush" Color="{StaticResource VerticalScrollBarButtonHoverOuterBorderBrushColor}" />

	<LinearGradientBrush x:Key="VerticalScrollBarButtonHoverInnerBorderBrush" StartPoint="0%,0%" EndPoint="100%,0%">
		<GradientStop Offset="0" Color="{StaticResource VerticalScrollBarButtonHoverInnerBorderBrushColor0}" />
		<GradientStop Offset="1" Color="{StaticResource VerticalScrollBarButtonHoverInnerBorderBrushColor1}" />
	</LinearGradientBrush>


	<LinearGradientBrush x:Key="VerticalScrollBarButtonPressedBackground" StartPoint="0%,0%" EndPoint="100%,0%">
		<GradientStop Offset="0" Color="{StaticResource VerticalScrollBarButtonPressedBackgroundColor0}" />
		<GradientStop Offset="0.5" Color="{StaticResource VerticalScrollBarButtonPressedBackgroundColor1}" />
		<GradientStop Offset="0.5" Color="{StaticResource VerticalScrollBarButtonPressedBackgroundColor2}" />
		<GradientStop Offset="1" Color="{StaticResource VerticalScrollBarButtonPressedBackgroundColor3}" />
