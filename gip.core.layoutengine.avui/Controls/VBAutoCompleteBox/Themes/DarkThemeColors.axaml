<ResourceDictionary
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   x:ClassModifier="internal">
  <!-- Color Resources -->
  <SolidColorBrush x:Key="AutoCompleteBoxBgEnabled" Color="#FF1A1A1A"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgEnabled" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgEnabledFocus" Color="#FFFFFFFF"/>
  <RadialGradientBrush x:Key="AutoCompleteBoxBorderEnabled" Center="0.85,0.9" RadiusX="0.9" GradientOrigin="0.8,0.8" RadiusY="0.8">
    <GradientStop Color="#FFC5C5C6" Offset="0.208"/>
    <GradientStop Color="#FF666666" Offset="0.548"/>
    <GradientStop Color="Black" Offset="1"/>
  </RadialGradientBrush>

  <SolidColorBrush x:Key="AutoCompleteBoxBgDisabled" Color="#FF2D2D30"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgDisabled" Color="Silver"/>
  <RadialGradientBrush x:Key="AutoCompleteBoxBorderDisabled" Center="0.8,0.8" RadiusX="1" GradientOrigin="0.7,0.8" RadiusY="0.8">
    <GradientStop Color="#FFC5C5C6" Offset="0.208"/>
    <GradientStop Color="#FF666666" Offset="0.548"/>
    <GradientStop Color="Black" Offset="1"/>
  </RadialGradientBrush>

  <SolidColorBrush x:Key="AutoCompleteBoxBgRequired" Color="#FF1A1A1A"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgRequired" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgRequiredFocus" Color="Yellow"/>
  <RadialGradientBrush x:Key="AutoCompleteBoxBorderRequired" Center="0.85,0.9" RadiusX="0.9" GradientOrigin="0.8,0.8" RadiusY="0.8">
    <GradientStop Color="#FFFFEA00" Offset="0.116"/>
    <GradientStop Color="#FFD1D261" Offset="0.3"/>
    <GradientStop Color="Black" Offset="1"/>
  </RadialGradientBrush>

  <SolidColorBrush x:Key="AutoCompleteBoxBgWrong" Color="#FF1A1A1A"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgWrong" Color="#FFFFFFFF"/>
  <SolidColorBrush x:Key="AutoCompleteBoxFgWrongFocus" Color="#FFFF0000"/>
  <RadialGradientBrush x:Key="AutoCompleteBoxBorderWrong" Center="0.85,0.9" RadiusX="0.9" GradientOrigin="0.8,0.8" RadiusY="0.8">
    <GradientStop Color="#FFFF0004" Offset="0.1116"/>
    <GradientStop Color="#FFE43D3D" Offset="0.3"/>
    <GradientStop Color="#FF020101" Offset="1"/>
  </RadialGradientBrush>

</ResourceDictionary>
