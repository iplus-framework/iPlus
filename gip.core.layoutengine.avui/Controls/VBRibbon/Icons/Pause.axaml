                            Fill="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:IVBDynamicIcon}}, Path=ContentFill}" 
                            >
                        </Path>
                        <Path Width="4" Height="16" Canvas.Left="14" Canvas.Top="4" Stretch="Fill" Data="F1 M 16,4L 16,4C 17.1046,4 18,4.89543 18,6L 18,18C 18,19.1046 17.1046,20 16,20L 16,20C 14.8954,20 14,19.1046 14,18L 14,6C 14,4.89543 14.8954,4 16,4 Z "
