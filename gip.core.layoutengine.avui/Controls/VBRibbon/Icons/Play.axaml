<ResourceDictionary xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:gip.core.layoutengine.avui">
    
    <ControlTheme x:Key="IconPlayStyle" TargetType="ContentControl">
        <Setter Property="Template">
            <ControlTemplate TargetType="ContentControl">
                <Viewbox Stretch="Uniform">
                    <Canvas Width="24" Height="24" Canvas.Left="0" Canvas.Top="0">
                        <Path Width="17" Height="17" Canvas.Left="3.49998" Canvas.Top="3.5" Stretch="Fill" Data="F1 M 20,12L 3.99998,20L 3.99998,4L 20,12 Z " 
                            StrokeStartLineCap="Round" StrokeEndLineCap="Round" StrokeLineJoin="Round"
                            Stroke="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:IVBDynamicIcon}}, Path=ContentStroke}" 
                            Fill="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:IVBDynamicIcon}}, Path=ContentFill}" 
                              >
                        </Path>
                    </Canvas>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>


