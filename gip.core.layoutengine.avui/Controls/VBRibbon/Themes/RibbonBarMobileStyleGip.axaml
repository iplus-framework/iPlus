<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:gip.core.layoutengine.avui">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/gip.core.layoutengine.avui;Component/Controls/Shared.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="RibbonBarMobileStyleGip" TargetType="{x:Type local:VBRibbonBarMobile}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:VBRibbonBarMobile}">
                    <Grid>
                        <Border>
                            <DockPanel>
                                <WrapPanel x:Name="WrapPanelItems" Orientation="Horizontal" MaxWidth="460" IsItemsHost="true" Margin="10,0,10,0">
                                </WrapPanel>
                            </DockPanel>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
