<Styles
   xmlns="https://github.com/avaloniaui"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:gip.core.visualcontrols.avui">
  <Styles.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XMotor.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XPump.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XPumpMembrane.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XPumpGear.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XVentilator.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XStarFeeder.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XChain.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XScrew.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XConveyor.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XElevator.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XSieve.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XScrewVert.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XFilter.axaml"/>
        <ResourceInclude Source="/Controls/VBMotor/Themes/XMixer.axaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Styles.Resources>


  <Style Selector="local|VBMotor[MotorType=StarFeeder]">
    <Setter Property="Theme" Value="{StaticResource StarFeeder}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Pump]">
    <Setter Property="Theme" Value="{StaticResource Pump}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=PumpMembrane]">
    <Setter Property="Theme" Value="{StaticResource PumpMembrane}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=PumpGear]">
    <Setter Property="Theme" Value="{StaticResource PumpGear}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Ventilator]">
    <Setter Property="Theme" Value="{StaticResource Ventilator}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Screw]">
    <Setter Property="Theme" Value="{StaticResource Screw}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Chain]">
    <Setter Property="Theme" Value="{StaticResource Chain}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Conveyor]">
    <Setter Property="Theme" Value="{StaticResource Conveyor}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=ElevatorBody]">
    <Setter Property="Theme" Value="{StaticResource ElevatorBody}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=ElevTopLeft]">
    <Setter Property="Theme" Value="{StaticResource ElevTopLeft}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=ElevTopRight]">
    <Setter Property="Theme" Value="{StaticResource ElevTopRight}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=ElevBottomLeft]">
    <Setter Property="Theme" Value="{StaticResource ElevBottomLeft}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=ElevBottomRight]">
    <Setter Property="Theme" Value="{StaticResource ElevBottomRight}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Sieve]">
    <Setter Property="Theme" Value="{StaticResource Sieve}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=ScrewVert]">
    <Setter Property="Theme" Value="{StaticResource ScrewVert}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Filter]">
    <Setter Property="Theme" Value="{StaticResource Filter}"/>
  </Style>
  <Style Selector="local|VBMotor[MotorType=Mixer]">
    <Setter Property="Theme" Value="{StaticResource Mixer}"/>
  </Style>

</Styles>
