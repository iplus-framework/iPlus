    <Style Selector="^ /template/ Viewbox#Group_0">
      <Style.Animations>
        <Animation IterationCount="Infinite" Duration="0:0:1" PlaybackDirection="Alternate">
          <KeyFrame Cue="0%">
            <Setter Property="RenderTransform" Value="rotate(-30deg)"/>
