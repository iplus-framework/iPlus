<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:gip.tool.installerAndUpdater"
        xmlns:tr="clr-namespace:gip.tool.installerAndUpdater.TextResources"
        x:Class="gip.tool.installerAndUpdater.MainWindow"
        Title="{x:Static tr:TextResource.TbProgramMaintHeader}" Height="400" Width="550" MaxHeight="450" MaxWidth="600" MinHeight="400" MinWidth="550"
        Icon="/gip.tool.installerAndUpdater;component/Images/gip.ico" 
        WindowStyle="SingleBorderWindow"
        WindowStartupLocation="CenterScreen"
        Background="#FF333333">
    <Grid>
        <TabControl x:Name="mainTabControl" Grid.Row="1" Grid.ColumnSpan="2" Background="{Binding RelativeSource={RelativeSource TemplatedParent}}" BorderBrush="Transparent" >
            <TabControl.ItemContainerStyle>
                <Style TargetType="{x:Type TabItem}">
                    <Setter Property="Visibility" Value="Collapsed" />
                </Style>
            </TabControl.ItemContainerStyle>
            <TabControl.Style>
                <Style TargetType="{x:Type TabControl}">
                    <Setter Property="Visibility" Value="Collapsed"/>
                    <Style.Triggers>
                        <DataTrigger Binding="{Binding ApplicationMode}" Value="0">
                            <Setter Property="Visibility" Value="Visible"/>
                        </DataTrigger>
                    </Style.Triggers>
                </Style>
            </TabControl.Style>
            <TabItem Header="Agreement">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                        <RowDefinition Height="40"/>
                    </Grid.RowDefinitions>

                    <Border Grid.ColumnSpan="5" BorderBrush="Red" BorderThickness="0,0,0,1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{x:Static tr:TextResource.TbLicenseAgreement}" Padding="10,0,0,0" Height="25" FontSize="18" Foreground="Red" />
                            <Image Grid.Column="1" Source="/gip.tool.installerAndUpdater;component/Images/gip.ico" HorizontalAlignment="Right" />
                        </Grid>
                    </Border>

                    <Grid Grid.Row="1" Grid.ColumnSpan="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="25" />
                            <ColumnDefinition/>
                            <ColumnDefinition Width="25" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="40" />
                            <RowDefinition />
                            <RowDefinition Height="50" />
                        </Grid.RowDefinitions>

                        <TextBlock VerticalAlignment="Center" Grid.Column="1" Text="{x:Static tr:TextResource.TbLicenseInfo}" Foreground="White" />
                        <TextBox x:Name="tbLicenseText" TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Background="Black" Foreground="White" BorderBrush="Transparent" IsReadOnly="True" 
							VerticalScrollBarVisibility="Auto" Text="{x:Static tr:TextResource.TbLicenseText}">
                            <TextBox.Style>
                                <Style TargetType="{x:Type TextBox}">
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate>
                                                <Border x:Name="Bd"
													BorderBrush="{TemplateBinding BorderBrush}" 
													BorderThickness="{TemplateBinding BorderThickness}"
													SnapsToDevicePixels="True"
													Background="{TemplateBinding Background}">
                                                    <ScrollViewer x:Name="PART_ContentHost"/>
                                                </Border>

                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </TextBox.Style>
                        </TextBox>
                        <TextBlock Grid.Row="2" Grid.Column="1" VerticalAlignment="Center" Foreground="White" TextWrapping="Wrap"
							Text="{x:Static tr:TextResource.TbLicenseInfo2}"/>
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <Button x:Name="btnAgree" Grid.Column="1" Grid.Row="2" Height="25" Click="btnAgree_Click" Content="{x:Static tr:TextResource.BtnIAgree}"/>
                    <Button x:Name="btnCancel" Grid.Column="3" Grid.Row="2" Height="25" Click="btnCancel_Click" Content="{x:Static tr:TextResource.BtnCancel}"/>
                </Grid>
            </TabItem>

            <TabItem Header="Login">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                        <RowDefinition Height="40"/>

                    </Grid.RowDefinitions>

                    <Border Grid.ColumnSpan="6" BorderBrush="Red" BorderThickness="0,0,0,1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="Login" Padding="10,0,0,0" Height="25" FontSize="18" Foreground="Red" />
                            <Image Grid.Column="1" Source="/gip.tool.installerAndUpdater;component/Images/gip.ico" HorizontalAlignment="Right" />
                        </Grid>
                    </Border>

                    <Grid Grid.Row="1" Grid.ColumnSpan="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1.2*" />
                            <ColumnDefinition Width="2*" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="80"/>
                            <RowDefinition Height="20"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="80"/>
                        </Grid.RowDefinitions>
                        <Image Source="/gip.tool.installerAndUpdater;Component/Images/keys.png" Grid.RowSpan="5" Height="150" Width="150" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        <TextBlock Grid.Column="1" TextWrapping="Wrap" Foreground="White" VerticalAlignment="Center"
                                   Text="{x:Static tr:TextResource.TbLoginInfo}" />
                        <TextBlock Grid.Row="2" Grid.Column="1" Text="{x:Static tr:TextResource.TbLoginEmail}" Foreground="White" VerticalAlignment="Center" />
                        <TextBox x:Name="tbEmail" Grid.Row="2" Grid.Column="1" Width="200" Height="20" HorizontalAlignment="Left" Margin="80,0,0,0" Background="Black" Foreground="White" />

                        <TextBlock Grid.Row="3" Grid.Column="1" Text="{x:Static tr:TextResource.TbLoginPassword}" Foreground="White" VerticalAlignment="Center" />
                        <PasswordBox x:Name="pbPassword" Grid.Row="3" Grid.Column="1" Width="200" Height="20" HorizontalAlignment="Left" Margin="80,0,0,0" Background="Black" Foreground="White" />

                        <TextBlock Grid.Row="4" Grid.Column="1" VerticalAlignment="Bottom" Foreground="White" Text="{x:Static tr:TextResource.TbLoginInfo2}" />
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <Button x:Name="btnBackLogin" Grid.Column="1" Grid.Row="2" Height="25" Click="btnBackLogin_Click" Content="{x:Static tr:TextResource.BtnBack}" />
                    <Button x:Name="btnLogin" Grid.Column="3" Grid.Row="2" Height="25" Click="btnLogin_Click" Content="{x:Static tr:TextResource.BtnLogin}" />
                    <Button x:Name="btnCancelLogin" Grid.Column="5" Grid.Row="2" Height="25" Click="btnCancelLogin_Click" Content="{x:Static tr:TextResource.BtnCancel}"/>
                </Grid>
            </TabItem>

            <TabItem Header="Installation">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                        <RowDefinition Height="40"/>

                    </Grid.RowDefinitions>

                    <Border Grid.ColumnSpan="6" BorderBrush="Red" BorderThickness="0,0,0,1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{x:Static tr:TextResource.TbInstallHeader}" Padding="10,0,0,0" Height="25" FontSize="18" Foreground="Red" />
                            <Image Grid.Column="1" Source="/gip.tool.installerAndUpdater;component/Images/gip.ico" HorizontalAlignment="Right" />
                        </Grid>
                    </Border>

                    <Grid Grid.Row="1" Grid.ColumnSpan="6">
                        <Grid Grid.Row="1" Grid.ColumnSpan="5">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="25" />
                                <ColumnDefinition/>
                                <ColumnDefinition Width="25" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="40" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="20" />
                                <RowDefinition Height="50" />
                                <RowDefinition Height="70" />
                                <RowDefinition Height="20" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="40" />
                            </Grid.RowDefinitions>

                            <TextBlock VerticalAlignment="Center" Grid.Row="0" Grid.Column="1" Foreground="White" TextWrapping="Wrap" 
								Text="{x:Static tr:TextResource.TbApplicationVersion}"/>

                            <TextBlock Grid.Row="1" Grid.Column="1" Foreground="White" Text="{x:Static tr:TextResource.CbAppVersion}" VerticalAlignment="Center" />
                            <ComboBox Grid.Row="1" Grid.Column="1" BorderBrush="Black" Width="200" Height="23" VerticalAlignment="Center" x:Name="cbiPlusVersion" 
                                      ItemContainerStyle="{DynamicResource ComboBoxItemStyle2}" Style="{DynamicResource ComboBoxStyle1}" IsEditable="False" HorizontalAlignment="Left" 
                                      Margin="90,0,0,0">
                                <ComboBoxItem Content="IPlusMES" IsSelected="True"/>
                                <ComboBoxItem Content="IPlus"/>
                            </ComboBox>

                            <Border Grid.Row="2" Grid.Column="1" BorderBrush="Gray" BorderThickness="0,0,0,1" />

                            <TextBlock VerticalAlignment="Center" Grid.Row="3" Grid.Column="1" Foreground="White" TextWrapping="Wrap" 
								Text="{x:Static tr:TextResource.TbInstallLocationInfo}"/>
                            <GroupBox Grid.Row="4" Grid.Column="1" Background="Transparent" BorderBrush="White" BorderThickness="0.3" >
                                <GroupBox.Header>
                                    <TextBlock Text="{x:Static tr:TextResource.TbDestinationFolder}" Foreground="White"/>
                                </GroupBox.Header>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition />
                                        <ColumnDefinition Width="10"/>
                                        <ColumnDefinition Width="80"/>
                                        <ColumnDefinition Width="10"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBox x:Name="tbInstallationFolder" Text="{Binding InstallationFolder}" Height="22" Grid.Column="1" Foreground="White" Background="Black" />
                                    <Button x:Name="btnBrowse" Height="22" Grid.Column="3" Content="{x:Static tr:TextResource.BtnBrowse}" Click="btnBrowse_Click" />
                                </Grid>
                            </GroupBox>

                            <CheckBox x:Name="ChbDesktopShortcut" Grid.Row="6" Grid.Column="1" Height="30" Content="{x:Static tr:TextResource.ChDesktopShortcut}" Foreground="White" 
                                      Style="{DynamicResource CheckBoxStyle1}" IsChecked="True" />

                        </Grid>
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <!--<Button x:Name="btnBackInstall" Grid.Column="1" Grid.Row="2" Height="25" Click="btnBackInstall_Click" Content="{x:Static tr:TextResource.BtnBack}"/>-->
                    <Button x:Name="btnInstall" Grid.Column="3" Grid.Row="2" Height="25" Click="btnInstall_Click" Content="{x:Static tr:TextResource.BtnInstall}"/>
                    <Button x:Name="btnCancelInstall" Grid.Column="5" Grid.Row="2" Height="25" Click="btnCancelInstall_Click" Content="{x:Static tr:TextResource.BtnCancel}" />
                </Grid>
            </TabItem>

            <TabItem Header="InstallationProgress">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                        <RowDefinition Height="40"/>

                    </Grid.RowDefinitions>

                    <Border Grid.ColumnSpan="5" BorderBrush="Red" BorderThickness="0,0,0,1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{x:Static tr:TextResource.TbInstallProgressHeader}" Padding="10,0,0,0" Height="25" FontSize="18" Foreground="Red" />
                            <Image Grid.Column="1" Source="/gip.tool.installerAndUpdater;component/Images/gip.ico" HorizontalAlignment="Right" />
                        </Grid>
                    </Border>

                    <Grid Grid.Row="1" Grid.ColumnSpan="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="30" />
                            <ColumnDefinition />
                            <ColumnDefinition Width="30" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="15" />
                            <RowDefinition Height="20" />
                            <RowDefinition />
                            <RowDefinition Height="20" />
                            <RowDefinition Height="23" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="20" />
                            <RowDefinition Height="23" />
                            <RowDefinition />
                            <RowDefinition Height="20" />
                            <RowDefinition Height="15" />
                        </Grid.RowDefinitions>
                        <TextBlock Grid.Column="1" Grid.Row="1" Foreground="White" Text="{x:Static tr:TextResource.TbInstallProgressInfo}" />

                        <TextBlock Grid.Column="1" Grid.Row="3" Text="{Binding CurrentOperationText}" Foreground="White" HorizontalAlignment="Left" />
                        <ProgressBar Grid.Row="4" Grid.Column="1" Minimum="0" Maximum="99" Height="23" Name="pbInstallStatus" Background="Black" BorderBrush="Black"
                                     Value="{Binding CurrentOperationProgress}" IsIndeterminate="{Binding IsIndeterminateCurrentOperationProgress}">
                            <ProgressBar.Style>
                                <Style TargetType="{x:Type ProgressBar}">
                                    <Setter Property="Foreground">
                                        <Setter.Value>
                                            <LinearGradientBrush StartPoint="0.5,-0.240642" EndPoint="0.5,1.24064">
                                                <LinearGradientBrush.GradientStops>
                                                    <GradientStop Color="#FFFC1300" Offset="0.00465116"/>
                                                    <GradientStop Color="#FFFD3D2E" Offset="0.418605"/>
                                                    <GradientStop Color="#FFFF685C" Offset="0.5"/>
                                                    <GradientStop Color="#FFFD3D2E" Offset="0.590698"/>
                                                    <GradientStop Color="#FFFC1300" Offset="1"/>
                                                </LinearGradientBrush.GradientStops>
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </ProgressBar.Style>
                        </ProgressBar>

                        <TextBlock Grid.Column="1" Grid.Row="6" Text="{x:Static tr:TextResource.TbInstallTotalProgress}" Foreground="White" HorizontalAlignment="Left" />
                        <ProgressBar Grid.Row="7" Grid.Column="1" Minimum="0" Maximum="99" Name="pbInstallTotalStatus" Background="Black" BorderBrush="Black"
                                     Value="{Binding TotalProgress}">
                            <ProgressBar.Style>
                                <Style TargetType="{x:Type ProgressBar}">
                                    <Setter Property="Foreground">
                                        <Setter.Value>
                                            <LinearGradientBrush StartPoint="0.5,-0.240642" EndPoint="0.5,1.24064">
                                                <LinearGradientBrush.GradientStops>
                                                    <GradientStop Color="#FFFC1300" Offset="0.00465116"/>
                                                    <GradientStop Color="#FFFD3D2E" Offset="0.418605"/>
                                                    <GradientStop Color="#FFFF685C" Offset="0.5"/>
                                                    <GradientStop Color="#FFFD3D2E" Offset="0.590698"/>
                                                    <GradientStop Color="#FFFC1300" Offset="1"/>
                                                </LinearGradientBrush.GradientStops>
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </ProgressBar.Style>
                        </ProgressBar>
                        
                        <TextBlock Grid.Column="1" Grid.Row="9" Text="{x:Static tr:TextResource.TbInstallProgressCompleted}" Foreground="White">
                            <TextBlock.Style>
                                <Style TargetType="{x:Type TextBlock}">
                                    <Setter Property="Visibility" Value="Hidden" />
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsInstallationFinished}" Value="True">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </TextBlock.Style>
                        </TextBlock>
                        
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <Button x:Name="btnNextInstallProgress" Grid.Column="1" Grid.Row="2" Height="25" Click="btnNextInstallProgress_Click" Content="{x:Static tr:TextResource.BtnNext}" >
                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="IsEnabled" Value="False" />
                                <Style.Triggers>
                                    <MultiDataTrigger>
                                        <MultiDataTrigger.Conditions>
                                            <Condition Binding="{Binding IsInstallationFinished}" Value="True" />
                                            <Condition Binding="{Binding IsCanceled}" Value="False" />
                                        </MultiDataTrigger.Conditions>
                                        <MultiDataTrigger.Setters>
                                            <Setter Property="IsEnabled" Value="True" />
                                        </MultiDataTrigger.Setters>
                                    </MultiDataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                    <Button x:Name="btnCancelInstallProgress" Grid.Column="3" Grid.Row="2" Height="25" Click="btnCancelInstallProgress_Click" Content="{x:Static tr:TextResource.BtnCancel}">
                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="IsEnabled" Value="True" />
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsCanceled}" Value="True">
                                        <Setter Property="IsEnabled" Value="False" />
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                </Grid>
            </TabItem>

            <TabItem Header="Sql server check">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                        <RowDefinition Height="40"/>

                    </Grid.RowDefinitions>

                    <Border Grid.ColumnSpan="6" BorderBrush="Red" BorderThickness="0,0,0,1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{x:Static tr:TextResource.TbSqlCheckHeader}" Padding="10,0,0,0" Height="25" FontSize="18" Foreground="Red" />
                            <Image Grid.Column="1" Source="/gip.tool.installerAndUpdater;component/Images/gip.ico" HorizontalAlignment="Right" />
                        </Grid>
                    </Border>

                    <Grid Grid.Row="1" Grid.ColumnSpan="6">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1.3*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="40"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition ></RowDefinition>
                        </Grid.RowDefinitions>
                        <Image Source="/gip.tool.installerAndUpdater;Component/Images/sqlserver.png" Height="150" Width="150" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        <TextBlock Height="45" VerticalAlignment="Bottom" Foreground="Gray" FontSize="10" Text="{x:Static tr:TextResource.TbMicrosoftTrademark}" 
                                   TextWrapping="Wrap" Margin="5,0,5,-15"/>
                        <Grid Grid.Column="1">
                            <Grid.Style>
                                <Style TargetType="{x:Type Grid}">
                                    <Setter Property="Visibility" Value="Collapsed"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding SqlBrowserServiceState}" Value="0">
                                            <Setter Property="Visibility" Value="Visible"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="20"></RowDefinition>
                                <RowDefinition Height="30"></RowDefinition>
                                <RowDefinition Height="2*" />
                                <RowDefinition Height="20"></RowDefinition>
                                <RowDefinition Height="25"></RowDefinition>
                                <RowDefinition Height="3*" />
                                <RowDefinition Height="35"></RowDefinition>
                                <RowDefinition Height="20"></RowDefinition>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1" Foreground="White" Text="{x:Static tr:TextResource.TbSqlCheckInfo}"  />
                            <TextBlock Grid.Row="3" Foreground="White" Text="{Binding SqlServerCheckInfo}"  />
                            <ProgressBar Grid.Row="4" Grid.Column="1" Grid.ColumnSpan="3" Height="23" Minimum="0" Maximum="100" IsIndeterminate="{Binding IsIndeterminate}" 
                                         Background="Black" BorderBrush="Black">
                                <ProgressBar.Style>
                                    <Style TargetType="{x:Type ProgressBar}">
                                        <Setter Property="Foreground">
                                            <Setter.Value>
                                                <LinearGradientBrush StartPoint="0.5,-0.240642" EndPoint="0.5,1.24064">
                                                    <LinearGradientBrush.GradientStops>
                                                        <GradientStop Color="#FFFC1300" Offset="0.00465116"/>
                                                        <GradientStop Color="#FFFD3D2E" Offset="0.418605"/>
                                                        <GradientStop Color="#FFFF685C" Offset="0.5"/>
                                                        <GradientStop Color="#FFFD3D2E" Offset="0.590698"/>
                                                        <GradientStop Color="#FFFC1300" Offset="1"/>
                                                    </LinearGradientBrush.GradientStops>
                                                </LinearGradientBrush>
                                            </Setter.Value>
                                        </Setter>
                                        <Style.Triggers>                                            
                                            <Trigger Property="IsIndeterminate" Value="False">
                                                <Setter Property="Value" Value="100"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </ProgressBar.Style>
                            </ProgressBar>
                            <TextBlock x:Name="SqlSearchCompleted" Grid.Row="6" Foreground="White" Text="{x:Static tr:TextResource.TbSqlCheckInfo2}" Visibility="{Binding IsSerachCompleted}"
                                       TextWrapping="Wrap"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.Style>
                                <Style TargetType="{x:Type Grid}">
                                    <Setter Property="Visibility" Value="Collapsed"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding SqlBrowserServiceState}" Value="10">
                                            <Setter Property="Visibility" Value="Visible"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="20"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition/>
                                <RowDefinition Height="50"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1" Text="{x:Static tr:TextResource.TbSqlCheckWarning}" Foreground="Red" FontSize="15" />
                            <TextBlock Grid.Row="3" Text="{x:Static tr:TextResource.TbSqlCheckWarningServiceNotRunning}" Foreground="White" VerticalAlignment="Top" />
                            <TextBlock Grid.Row="5" Foreground="White" VerticalAlignment="Top" TextWrapping="Wrap"
                                       Text="{x:Static tr:TextResource.TbSqlCheckWarningServiceNotRunningInfo}"/>
                        </Grid>
                        <Grid Grid.Column="1">
                            <Grid.Style>
                                <Style TargetType="{x:Type Grid}">
                                    <Setter Property="Visibility" Value="Collapsed"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding SqlBrowserServiceState}" Value="20">
                                            <Setter Property="Visibility" Value="Visible"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="10"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="20"/>
                                <RowDefinition Height="25"/>
                                <RowDefinition Height="15"/>
                                <RowDefinition/>
                                <RowDefinition Height="50"/>
                            </Grid.RowDefinitions>
                            <TextBlock Grid.Row="1" Text="{x:Static tr:TextResource.TbSqlCheckWarning}" Foreground="Red" FontSize="15" />
                            <TextBlock Grid.Row="3" Text="{x:Static tr:TextResource.TbSqlCheckWarningServiceMissing}" Foreground="White" VerticalAlignment="Top" />
                            <TextBlock Grid.Row="5" Foreground="White" VerticalAlignment="Top" TextWrapping="Wrap"
                                       Text="{x:Static tr:TextResource.TbSqlCheckWarningServiceMissingInfo}"/>
                        </Grid>
                    </Grid>
                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <Button x:Name="btnRefreshSql" Grid.Column="1" Grid.Row="2" Height="25" Click="btnRefreshSql_Click" Content="{x:Static tr:TextResource.BtnRefresh}"
                            IsEnabled="{Binding IsNextButtonEnabled}">
                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding SqlBrowserServiceState}" Value="10">
