                        <TextBlock Grid.Column="3" Grid.Row="8" Foreground="White" Text="{x:Static tr:TextResource.TbUninstallInfo}" VerticalAlignment="Bottom" TextWrapping="Wrap" />
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <Button x:Name="btnUninstall"  Grid.Column="1" Grid.Row="2" Height="25" Click="btnUninstall_Click" Content="{x:Static tr:TextResource.BtnUninstall}">
                    </Button>
                    <Button x:Name="btnUninstallCancel" Grid.Column="3" Grid.Row="2" Height="25" Click="btnUninstallCancel_Click" Content="{x:Static tr:TextResource.BtnCancel}">
                    </Button>
                </Grid>
            </TabItem>
            <TabItem x:Name="TabItemUnistallProgress" Header="UninstallProgress">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="5"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40"/>
                        <RowDefinition/>
                        <RowDefinition Height="40"/>
                    </Grid.RowDefinitions>

                    <Border Grid.ColumnSpan="5" BorderBrush="Red" BorderThickness="0,0,0,1">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="20"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="{x:Static tr:TextResource.TbUninstallProgressHeader}" Padding="10,0,0,0" Height="25" FontSize="18" Foreground="Red" />
                            <Image Grid.Column="1" Source="/gip.tool.installerAndUpdater;component/Images/gip.ico" HorizontalAlignment="Right" />
                        </Grid>
                    </Border>

                    <Grid Grid.Row="1" Grid.ColumnSpan="5">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="25" />
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="25" />
                            <ColumnDefinition Width="2.5*"/>
                            <ColumnDefinition Width="25" />
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="10" />
                            <RowDefinition Height="20" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="55" />
                            <RowDefinition Height="20" />
                            <RowDefinition Height="25" />
                            <RowDefinition Height="25" />
                            <RowDefinition  />
                            <RowDefinition Height="40" />
                            <RowDefinition Height="10" />
                        </Grid.RowDefinitions>
                        <Image Grid.Column="1" Grid.Row="3" Grid.RowSpan="6" HorizontalAlignment="Center" Margin="0,0,0,20"  Source="/gip.tool.installerAndUpdater;component/Images/uninstallLarge.png" />
                        <TextBlock Grid.Column="3" Grid.Row="1" Foreground="White" FontSize="14" Text="{x:Static tr:TextResource.TbUninstallProgressInfo}" />

                        <TextBlock Grid.Column="3" Grid.Row="4" Foreground="White" Margin="0,0,0,5" VerticalAlignment="Bottom" Text="{x:Static tr:TextResource.TbUninstallProgressHeader}"/>
                        <ProgressBar Grid.Column="3" Grid.Row="5" Minimum="0" Maximum="100" Background="Black" BorderBrush="Black" Value="{Binding UninstallProgress}" >
                            <ProgressBar.Style>
                                <Style TargetType="{x:Type ProgressBar}">
                                    <Setter Property="Foreground">
                                        <Setter.Value>
                                            <LinearGradientBrush StartPoint="0.5,-0.240642" EndPoint="0.5,1.24064">
                                                <LinearGradientBrush.GradientStops>
                                                    <GradientStop Color="#FFFC1300" Offset="0.00465116"/>
                                                    <GradientStop Color="#FFFD3D2E" Offset="0.418605"/>
                                                    <GradientStop Color="#FFFF685C" Offset="0.5"/>
                                                    <GradientStop Color="#FFFD3D2E" Offset="0.590698"/>
                                                    <GradientStop Color="#FFFC1300" Offset="1"/>
                                                </LinearGradientBrush.GradientStops>
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </ProgressBar.Style>
                        </ProgressBar>

                        <TextBlock Grid.Column="3" Grid.Row="8" Foreground="White" Text="{x:Static tr:TextResource.TbUninstallProgressInfo2}" VerticalAlignment="Bottom" TextWrapping="Wrap">
                            <TextBlock.Style>
                                <Style TargetType="{x:Type TextBlock}">
                                    <Setter Property="Visibility" Value="Collapsed" />
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsUninstallCompleted}" Value="True">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </TextBlock.Style>
                        </TextBlock>
                    </Grid>

                    <TextBlock Grid.Column="0" Grid.Row="2" Text="{x:Static tr:TextResource.TbAllRightsReserved}" Foreground="Red" VerticalAlignment="Bottom" />
                    <Button x:Name="btnUninstallFinish" Grid.Column="3" Grid.Row="2" Height="25" Click="btnUninstallFinish_Click" Content="{x:Static tr:TextResource.BtnFinish}">
                        <Button.Style>
                            <Style TargetType="{x:Type Button}">
                                <Setter Property="Visibility" Value="Collapsed"/>
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsUninstallCompleted}" Value="True">
                                        <Setter Property="Visibility" Value="Visible"/>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                </Grid>
            </TabItem>

        </TabControl>

    </Grid>
</Window>
