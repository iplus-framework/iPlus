<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <PackageReference Include="Avalonia.BuildServices">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <Analyzer Include="$(AvaloniaRootDir)/src/tools/Avalonia.Analyzers/bin/$(Configuration)/netstandard2.0/Avalonia.Analyzers.dll">
      <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
      <OutputItemType>Analyzer</OutputItemType>
    </Analyzer>
    <Analyzer Include="$(AvaloniaRootDir)/src/tools/Avalonia.Generators/bin/$(Configuration)/netstandard2.0/Avalonia.Generators.dll">
      <ReferenceOutputAssembly>false</ReferenceOutputAssembly>
      <OutputItemType>Analyzer</OutputItemType>
    </Analyzer>
  </ItemGroup>
</Project> 