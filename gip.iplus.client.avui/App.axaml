<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vb="http://www.iplus-framework.com/axaml" 
             xmlns:local="using:gip.iplus.client.avui"
             x:Class="gip.iplus.client.avui.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceInclude Source="avares://gip.core.layoutengine.avui/Controls/DarkThemeColors.axaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
    <Application.Styles>
      <vb:IPlusTheme />
    </Application.Styles>
</Application>